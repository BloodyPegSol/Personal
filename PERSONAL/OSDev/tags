$(BUILD_DIR)/boot.bin	Makefile	/^$(BUILD_DIR)\/boot.bin: always$/;"	t
$(BUILD_DIR)/kernel.bin	Makefile	/^$(BUILD_DIR)\/kernel.bin: always$/;"	t
$(BUILD_DIR)/main_floppy.img	Makefile	/^$(BUILD_DIR)\/main_floppy.img: kernel bootloader$/;"	t
ASM	Makefile	/^ASM=nasm$/;"	m
BOOT_DIR	Makefile	/^BOOT_DIR=src\/bootloader$/;"	m
BUILD_DIR	Makefile	/^BUILD_DIR=build$/;"	m
KERNEL_DIR	Makefile	/^KERNEL_DIR=src\/kernel$/;"	m
SRC_DIR	Makefile	/^SRC_DIR=src$/;"	m
always	Makefile	/^always:$/;"	t
bdb_bytes_per_sector	src/bootloader/boot.asm	/^bdb_bytes_per_sector:		dw 512		; $/;"	d
bits	src/bootloader/boot.asm	/^bits 16$/;"	l
bits	src/kernel/kernel.asm	/^bits 16$/;"	l
bodb_oem	src/bootloader/boot.asm	/^bodb_oem:			db 'MSWIN4.1'	; 8 bytes$/;"	d
bootloader	Makefile	/^bootloader: $(BUILD_DIR)\/boot.bin$/;"	t
clean	Makefile	/^clean:$/;"	t
done	src/bootloader/boot.asm	/^.done:$/;"	l
done	src/kernel/kernel.asm	/^.done:$/;"	l
dw	src/bootloader/boot.asm	/^dw 0AA55h$/;"	l
dw	src/kernel/kernel.asm	/^dw 0AA55h$/;"	l
floppy_image	Makefile	/^floppy_image: $(BUILD_DIR)\/main_floppy.img$/;"	t
halt	src/bootloader/boot.asm	/^.halt:$/;"	l
halt	src/kernel/kernel.asm	/^.halt:$/;"	l
jmp	src/bootloader/boot.asm	/^jmp short start$/;"	l
kernel	Makefile	/^kernel: $(BUILD_DIR)\/kernel.bin$/;"	t
loop	src/bootloader/boot.asm	/^.loop:$/;"	l
loop	src/kernel/kernel.asm	/^.loop:$/;"	l
main	src/bootloader/boot.asm	/^main:$/;"	l
main	src/kernel/kernel.asm	/^main:$/;"	l
mov	src/bootloader/boot.asm	/^	mov ds, ax$/;"	d
mov	src/kernel/kernel.asm	/^	mov ds, ax$/;"	d
msg_hello	src/bootloader/boot.asm	/^msg_hello: db 'Hello world!', ENDL, 0$/;"	d
msg_hello	src/kernel/kernel.asm	/^msg_hello: db 'Hello world!', ENDL, 0$/;"	d
nop	src/bootloader/boot.asm	/^nop$/;"	l
org	src/bootloader/boot.asm	/^org 0x7C00$/;"	l
org	src/kernel/kernel.asm	/^org 0x7C00$/;"	l
puts	src/bootloader/boot.asm	/^puts:$/;"	l
puts	src/kernel/kernel.asm	/^puts:$/;"	l
start	src/bootloader/boot.asm	/^start:$/;"	l
start	src/kernel/kernel.asm	/^start:$/;"	l
times	src/bootloader/boot.asm	/^times 510-($-$$) db 0$/;"	l
times	src/kernel/kernel.asm	/^times 510-($-$$) db 0$/;"	l
